    "format": "{icon}",
    "format-icons": ["Û∞Éû ", "Û∞Éü ", "Û∞Éù ", "Û∞É† "],
    "tooltip": true,
    "tooltip-format": "{percent}%"
  },
  "network#speed": {
    "interval": 1,
    "format": "{ifname}%%",
    "format-wifi": "ÔÅ£ {bandwidthDownBytes} ÔÅ¢ {bandwidthUpBytes}",
    "format-ethernet": "ÔÑ∫ {bandwidthDownBytes} ÔÑ∫ {bandwidthUpBytes} ",
