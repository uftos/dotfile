{
  "layer": "top",
  "margin-left": 3,
  "margin-right": 3,
  "margin-top": 2,
  "margin-bottom": 0,
  "modules-left": ["cpu", "memory", "disk", "network#speed", "temperature"],
  "modules-center": ["clock"],
  "modules-right": [
    "pulseaudio/slider",
    "backlight",
    "network",
    "battery"
