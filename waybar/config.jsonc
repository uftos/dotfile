},
  "custom/weather": {
    "format": "{}¬∞C",
    "tooltip": false,
    "interval": 3600,
    "exec": "wttrbar --location 'Ahmedabad'",
    "return-type": "json"
  },
  "clock": {
    "format": "{:%H:%M | %a %d/%m}",
    "tooltip": true,
    "tooltip-format": "<tt>{calendar}</tt>"
  },
  "network": {
    "format-wifi": " {icon}",
    "format-ethernet": "Û∞àÄ",
    "format-icons": ["Û∞§Ø ", "Û∞§ü ", "Û∞§¢ ", "Û∞§¢ ", "Û∞§® "],
