  "pulseaudio/slider": {
    "min": 0,
    "max": 100,
    "orientation": "horizontal"
  },
  "custom/cava": {
    "format": "{}",
    "exec": "~/.config/waybar/scripts/cava.sh",
    "on-click": "kitty cava"
  },
  "custom/update": {
    "format": "{}",
    "exec": "~/.config/waybar/scripts/check-updates.sh"
    // "on-click": "kitty cava"
  },
